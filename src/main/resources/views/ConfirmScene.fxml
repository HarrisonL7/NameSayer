            <Image url="@images/warning.png" />
         </image>
      </ImageView>
      <Button fx:id="_yesButton" layoutX="89.0" layoutY="123.0" mnemonicParsing="false" onAction="#yesButtonAction" prefHeight="30.0" prefWidth="60.0" text="Yes" textFill="WHITE" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="80.0">
