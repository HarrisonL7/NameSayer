            <String fx:value="menu-dark" />
            <String fx:value="smallgreenbutton" />
         </styleClass></Button>
      <Button fx:id="_noButton" layoutX="170.0" layoutY="123.0" mnemonicParsing="false" onAction="#noButtonAction" prefHeight="30.0" prefWidth="60.0" text="No" textFill="WHITE" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="80.0">
         <styleClass>
            <String fx:value="smallredbutton" />
            <String fx:value="menu-dark" />
         </styleClass></Button>
   </children>
</AnchorPane>
