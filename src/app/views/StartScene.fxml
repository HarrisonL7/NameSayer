<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: grey;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.StartSceneController">
   <children>
      <Label layoutX="143.0" layoutY="91.0" text="Welcome to Name Sayer" textFill="WHITE">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <Button layoutX="252.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleStartAction" prefHeight="50.0" prefWidth="100.0" text="Start" textFill="WHITE">
         <styleClass>
            <String fx:value="menu" />
            <String fx:value="play" />
         </styleClass>
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button layoutX="252.0" layoutY="271.0" mnemonicParsing="false" onAction="#handleQuitAction" prefHeight="50.0" prefWidth="100.0" text="Quit">
         <styleClass>
            <String fx:value="menu" />
            <String fx:value="bad" />
         </styleClass>
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
